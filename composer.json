{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "proprietary",
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.cartalyst.com"
		}
	],
	"require": {
		"laravel/framework": "5.0.*@dev",
		"illuminate/html": "5.0.*",
		"illuminate/filesystem": "5.0.*",
		"illuminate/support": "5.0.*",
		"doctrine/dbal": "~2.5",
		"predis/predis": "~1.0",
		"league/glide": "~0.3.0",
		"intervention/imagecache": "~2.1.1",
		"cartalyst/sentinel": "dev-feature/laravel-5",
		"cartalyst/sentinel-social": "dev-feature/laravel-5",
		"cartalyst/sentinel-unique-passwords": "dev-feature/laravel-5"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"way/generators": "~3.0",
		"itsgoingd/clockwork": "dev-master",
		"fzaninotto/faker": "~1.5.0",
		"league/flysystem-dropbox": "1.0.*@dev",
		"league/flysystem-ziparchive": "1.0.*@dev",
		"league/flysystem-cached-adapter": "1.0.*@dev"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		},
		"files": [
			"resources/Helpers.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}